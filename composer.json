{
    "name": "amdadulhaq/cloudflared-valet",
    "description": "A Laravel package to create and manage Cloudflare Tunnels for local development",
    "homepage": "https://github.com/amdad121/laravel-cloudflared-valet",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Aerni",
            "email": "hello@michaelaerni.ch",
            "homepage": "https://michaelaerni.ch",
            "role": "Developer"
        },
        {
            "name": "Amdadul Haq",
            "email": "amdaddev121@gmail.com",
            "homepage": "https://amdadulhaq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "cloudflare/sdk": "^1.4",
        "illuminate/support": "^11.0 || ^12.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Aerni\\Cloudflared\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aerni\\Cloudflared\\CloudflaredServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
